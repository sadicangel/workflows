    <PackageReference Include="Elsa.Activities.Temporal.Quartz" Version="2.11.1" />
    <PackageReference Include="Elsa.Designer.Components.Web" Version="2.11.1" />
    <PackageReference Include="Elsa.Persistence.EntityFramework.Sqlite" Version="2.11.1" />
    <PackageReference Include="Elsa.Server.Api" Version="2.11.1" />
    <PackageReference Include="Humanizer.Core" Version="2.14.1" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.18.1" />
  </ItemGroup>

</Project>
